.historyPunchCard .search{
  margin-left: 30rpx;
  margin-right: 30rpx;
  border-radius: 34rpx;
  background-color: #f5f5f5;
  margin-top: 14rpx;
  margin-bottom: 14rpx;
  display:flex;
  align-items:center;
}
.historyPunchCard .search .iconSearch{
  float: left;
  margin-left: 28rpx;
}
.historyPunchCard .search input{
  font-family: "PingFangSC-Regular";
  font-size: 23rpx;
  color: #aaa9ae;
  float: left;
}
.historyPunchCard .stage{
  position: relative;
  border-top: 20rpx solid #f1f2f6;
  /*padding-left: 30rpx;*/
}
.historyPunchCard .stageHead{
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1rpx solid #f1f2f6;
  margin-left: 30rpx;
  font-family: "PingFangSC-Medium";
  font-size: 32rpx;
  color: #0f161e;
}
.historyPunchCard .stageImg{
  width: 200rpx;
  height: 150rpx;
  border: 1rpx solid red;
  float: left;
  margin-left: 30rpx;
}
.historyPunchCard .changeLocation{
  width: 42rpx;
  height: 42rpx;
  float: left;
  margin-left: 50rpx;
  margin-right: 10rpx;
}
.historyPunchCard .operation {
  float: left;
}
.historyPunchCard .operation image{
  width: 42rpx;
  height: 42rpx;
  margin-left: 170rpx;
}
.historyPunchCard .operation .edit{
  margin-left: 70rpx;
}
.historyPunchCard .stageContent{
  float: left;
}
.historyPunchCard .stageContent-name text{
  font-size: 32rpx;
}
.historyPunchCard .stageContent-content text{
  font-size: 28rpx;
  color: #aaa9ae;
}
.historyPunchCard .stageContent-name text,.historyPunchCard .stageContent-content text{
  margin-left: 30rpx;
}
.historyPunchCard .choise{
  color: #4286f5;
  border: 1rpx solid #4286f5;
  background-color: white;
  width: 78rpx;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 20rpx;
  padding: 0;
  margin-left: 30rpx;
  margin-top: 16rpx;
}