}
.projectList .footer .text{
  padding-top:62rpx;
  font-size: 32rpx;
  /*margin-right: 20rpx;*/
  /*display: block;*/
  float: left;
  margin-left: 30rpx;

}
.projectList .footer .addNew{
  margin-left:30rpx;
  margin-right: 30rpx;
  margin-top: 62rpx;
