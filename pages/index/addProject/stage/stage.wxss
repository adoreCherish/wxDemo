.all{
	border-top: 20rpx solid #f1f2f6;
	/*border-bottom: 20rpx solid #f1f2f6;*/

}
.all .header{
	height: 92rpx;
	border-bottom: 1rpx solid #e5e5e5;
	line-height: 92rpx;
}
.all .icon{
	width:37rpx;
	height: 42rpx;
	margin-left: 30rpx;
	margin-right: 30rpx;
}
.all  .title{
	font-family: "PingFangSC-Medium";
	color: #0f161e;
	font-size: 32rpx;
}
.all .content .stage{
	margin-left: 30rpx;
	margin-top: 30rpx;
	display: block;
	font-size: 28rpx;
}
.main{
	margin-left: 30rpx;
	margin-right: 30rpx;
	margin-top: 20rpx;
	box-shadow:0 0 20rpx 20rpx #F6F9FE;
}
.wrapper{
	margin-left: 30rpx;
	margin-right: 30rpx;
	border-bottom: 1rpx solid #f1f2f6;
	overflow: hidden;
	line-height: 112rpx;
	height: 112rpx;
}
.wrapper .label{
	padding-right: 20rpx;
	font-family: "PingFangSC-Regular";
	color: #0f161e;
	float: left;
	font-size: 32rpx;
	margin-left: 10rpx;
}
.wrapper input{
	font-family: "PingFangSC-Regular";
	color: #aaa9ae;
	float: left;
	margin-top:30rpx;
	font-size: 32rpx;
}
.wrapper .picker .text{
	/*margin-left: 10rpx;*/
	font-size: 32rpx;
	color:#aaa9ae;
	float: left;
}
.wrapper .radio{
	font-size: 32rpx;
	font-family: "PingFangSC-Regular";
	float: left
}
.wrapper radio{
	/*width:30rpx;*/
	/*height:30rpx;*/
	margin-right:10rpx;
}
.content .addStage{
	width:160rpx;
	height: 60rpx;
	margin-top: 40rpx;
	margin-left: 60rpx;
	margin-bottom: 40rpx;
	font-size: 28rpx;
	font-family: "PingFangSC-Regular";
	color: #3eb85f;
	border-radius: 12rpx;
	border: 1rpx solid #3eb85f;
	background-color: white;
	padding: 0;
	line-height: 60rpx;
}
.main .navigator {
  padding: 20rpx 30rpx;
  position: relative;
  display: flex;
  align-items: center;
  float: right;
  margin-top:35rpx;
}
.main .navigator:before {
  content: " ";
  position: absolute;
  left: 30rpx;
  top: 0;
  right: 30rpx;
  height: 1px;
  border-top: 1rpx solid #D8D8D8;
  color: #D8D8D8;
}
.main .navigator:first-child:before {
  display: none;
}
.navigator-text {
  /*flex: 1;*/
}
.navigator-arrow {
  /*padding-right: 26rpx;
  /*position: relative;*/
}
.main .navigator-arrow:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #888888;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  /*right: 28rpx;*/
  right:4rpx;
}
.publish{
	margin-top: 20rpx;
	height: 92rpx;
	border-radius: 5rpx;
	background-color: #3bb35b;
	font-size: 36rpx;
	font-family: "PingFangSC-Regular";
	margin-bottom: 30rpx;
	margin-left: 30rpx;
	margin-right: 30rpx;
}